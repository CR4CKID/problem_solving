   "source": [
    "from collections import Counter\n",
    "n = int(input())\n",
    "nums = list(map(int, input().split(' ')))\n",
    "counter = Counter(nums)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3 0 3 0 3 0\n"
     ]
    }
   ],
   "source": [
    "ans = []\n",
    "for num in nums:\n",
    "    ans.append(sum([c[1] for c in counter.items() if c[0] < num]))\n",
    "print(' '.join(map(str, ans)))"
   ]
  }
